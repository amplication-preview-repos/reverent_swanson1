datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Template {
  createdAt       DateTime               @default(now())
  createdBy       String?
  file            Json?
  generatedImages Json?
  generatedPages  Json?
  id              String                 @id @default(cuid())
  name            String?
  typeField       EnumTemplateTypeField?
  updatedAt       DateTime               @updatedAt
}

enum EnumTemplateTypeField {
  Option1
}
